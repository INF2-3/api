{
  "$schema":"https://json-schema.org/draft-07/schema",
  "$id":"http://main.com/schema.json",
  "title":"Bank statements",
  "type":"object",
  "required":[
    "bankStatements"
  ],
  "properties":{
    "bankStatements":{
      "$id":"#root/bankStatements",
      "title":"Bankstatements",
      "type":"array",
      "default":[

      ],
      "items":{
        "$id":"#root/bankStatements/items",
        "title":"Items",
        "type":"object",
        "required":[
          "id",
          "accountNumber",
          "uploadDate",
          "closingBalance",
          "uploader"
        ],
        "properties":{
          "id":{
            "$id":"#root/bankStatements/items/id",
            "title":"Id",
            "type":"integer",
            "examples":[
              1
            ],
            "default":0
          },
          "accountNumber":{
            "$id":"#root/bankStatements/items/accountNumber",
            "title":"Accountnumber",
            "type":"string",
            "default":"",
            "examples":[
              "NL69INGB0123456789EUR"
            ],
            "pattern":"^.*$"
          },
          "uploadDate":{
            "$id":"#root/bankStatements/items/uploadDate",
            "title":"Uploaddate",
            "type":"string",
            "default":"",
            "examples":[
              "140220"
            ],
            "pattern":"^.*$"
          },
          "closingBalance":{
            "$id":"#root/bankStatements/items/closingBalance",
            "title":"Closingbalance",
            "type":"number",
            "examples":[
              555.3
            ],
            "default":0.0
          },
          "uploader":{
            "$id":"#root/bankStatements/items/uploader",
            "title":"Uploader",
            "type":"string",
            "default":"",
            "examples":[
              "joe"
            ],
            "pattern":"^.*$"
          }
        }
      }
    }
  }
}